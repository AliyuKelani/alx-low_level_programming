#include "main.h"

/**
 * _abs - returns the absolute value of a number
 *@i: number whose absolute value is returned
 *
 * Return: absolute value of i
 */
int _abs(int i)
{
	int res;

	if (i < 0)
		res = i * -1;
	else )
	{
		res = 1;
		_putchar('+');
	}
	else
	{
		res = 0;
		_putchar('0');
	}
	return (res);
}
